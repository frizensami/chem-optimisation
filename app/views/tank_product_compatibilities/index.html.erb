<%- model_class = TankProductCompatibility -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize.titleize %></h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:id) %></th>
      <th><%= model_class.human_attribute_name(:tank_id) %></th>
      <th><%= model_class.human_attribute_name(:product_id) %></th>
      <th><%= model_class.human_attribute_name(:created_at) %></th>
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @tank_product_compatibilities.each do |tank_product_compatibility| %>
      <tr>
        <td><%= link_to tank_product_compatibility.id, tank_product_compatibility_path(tank_product_compatibility) %></td>
        <td><%= tank_product_compatibility.tank_id %></td>
        <td><%= tank_product_compatibility.product_id %></td>
        <td><%=l tank_product_compatibility.created_at %></td>
        <td>
          <%= link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_tank_product_compatibility_path(tank_product_compatibility), :class => 'btn btn-default btn-xs' %>
          <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
                      tank_product_compatibility_path(tank_product_compatibility),
                      :method => :delete,
                      :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
                      :class => 'btn btn-xs btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to t('.new', :default => t("helpers.links.new")),
            new_tank_product_compatibility_path,
            :class => 'btn btn-primary' %>
