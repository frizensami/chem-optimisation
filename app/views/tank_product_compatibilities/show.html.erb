<%- model_class = TankProductCompatibility -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:tank_id) %>:</strong></dt>
  <dd><%= @tank_product_compatibility.tank_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:product_id) %>:</strong></dt>
  <dd><%= @tank_product_compatibility.product_id %></dd>
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              tank_product_compatibilities_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_tank_product_compatibility_path(@tank_product_compatibility), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              tank_product_compatibility_path(@tank_product_compatibility),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
